<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>DynamicWebProject_01</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>Controller2</servlet-name>
		<servlet-class>controller.Controller</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Controller</servlet-name>
		<url-pattern>/Controller/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ControllerP</servlet-name>
		<servlet-class>controller.Controller2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ControllerP</servlet-name>
		<url-pattern>/ControllerP/*</url-pattern>
	</servlet-mapping>
</web-app>

